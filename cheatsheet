# example of object with methods
const pet = {
  name: 'Toby',
  typeOfPet: 'Dog',
  age: 7,
  colour: 'Blue',

  eat: function () {
    console.log(this.name + ' is eating');
  },
  drink: function () {
    console.log(this.name + ' is drinking');
  }

}

pet.eat()
pet.drink();





